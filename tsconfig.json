{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "DOM",
      "ES6"
    ],
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "@config": [
        "./config/index.ts"
      ],
      "@api/*": [
        "./api/*"
      ],
    },
    // "isolatedModules": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  // "include": [
  //   "src/**/*",
  //   "src/api/v1/**/*"
  // ],
  "exclude": [
    "node_modules"
  ]
}